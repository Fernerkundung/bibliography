isbn = {1002-0063},
issn = {1002-0063},
journal = {Chinese Geographical Science},
keywords = {agricultural intensification,agro-meteorological observation,china,crop calendar,moderate resolution imaging spectroradiometer (MOD,multiple-cropping,rice},
