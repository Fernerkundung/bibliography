issn = {2072-4292},
journal = {Remote Sensing},
keywords = {HJ-1A/B CCD images,classification,phenology,rice fields mapping},
month = {mar},
number = {4},
pages = {3467--3488},
title = {{Rice Fields Mapping in Fragmented Area Using Multi-Temporal HJ-1A/B CCD Images}},
