issn = {18735991},
journal = {World Development},
keywords = {family farm,farm size,farmland distribution,small farm,smallholder},
pages = {16--29},
title = {{The Number, Size, and Distribution of Farms, Smallholder Farms, and Family Farms Worldwide}},
url = {http://dx.doi.org/10.1016/j.worlddev.2015.10.041},
volume = {87},
year = {2016}
